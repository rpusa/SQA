#include <iostream.h>
#include <conio.h>
void main() {
   int a, b, c;
   
   a = 10;
   b = 20;
   
   c = a + b;   
   cout<<"Value of c = \n";
   cout<<c;
   
   c = a - b;   
   cout<<"Value of c = \n"
   cout<<c;
   
   c = a * b;   
   cout<<"Value of c = \n"
   cout<<c;
   
   c = b / a;   
   cout<<"Value of c = \n"
   cout<<c;
   
   c = b % a;   
   cout<<"Value of c = \n"
   cout<<c; 
   getch();
}
